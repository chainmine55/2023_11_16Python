   "source": [
    "#三種條件範例1-1\n",
    "#請輸入學生分數0-100分\n",
    "#0-59分不及格\n",
    "#60-79分及格\n",
    "#80-100優\n",
    "\n",
    "import pyinputplus as pyip\n",
    "\n",
    "scores = pyip.inputInt(\"請輸入學生分數(1-100:)\",min=0,max=100)\n",
    "print(scores)\n",
    "\n",
    "if scores < 60:\n",
    "    print(\"不及格\")\n",
    "else:\n",
    "    if scores < 80:\n",
    "        print(\"及格\")\n",
    "    else:\n",
    "        print(\"優\")\n",
    "\n"
   ]
  }
